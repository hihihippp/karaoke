<% content_for :title do %>
  Home
<% end %>

<% content_for :header do %>
  <script type="text/javascript">
	$(function() {
		$("ul#hover span").css("opacity","0");
		$("ul#hover span").hover(function () {
			$(this).stop().animate({
				opacity: 1
			});
		},
		function () {
			$(this).stop().animate({
				opacity: 0
			});
		});
	});
</script>
<% end %>

<div id="container">


  <div id="homeheader">
    <img src="assets/title2.png"/>
  </div>
  <!-- homeheader -->
  <div class="separator">
  </div>
  <!-- seperator -->

  <div id="videoframe">

    <%= render :partial => 'youtube' %>

  </div>
  <!-- videoframe -->

  <div id="practice">

    <ul id="hover">
      <li><a href="<%= page2_path %>" class="soundboothglow"><span></span></a></li>
    </ul>

  </div>

</div><!-- container -->





